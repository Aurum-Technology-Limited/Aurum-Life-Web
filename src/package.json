{
  "name": "aurum-life",
  "version": "1.0.0",
  "description": "Personal Operating System for Life Management",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern='test' --coverage",
    "test:phase4": "jest --testPathPattern='phase4'",
    "test:accessibility": "jest --testPathPattern='accessibility'",
    "test:visual": "jest --testPathPattern='visual'",
    "test:integration": "jest --testPathPattern='integration'",
    "test:enhanced": "node tests/enhanced-test-runner.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "audit": "npm audit --audit-level=moderate",
    "analyze": "npm run build && npx bundle-analyzer dist/assets/*.js",
    "performance": "npm run build && npx lighthouse http://localhost:4173 --chrome-flags='--headless' --output-path=./performance-report.html",
    "test:all": "npm run test:enhanced"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "zustand": "^4.4.1",
    "motion": "^10.16.0",
    "lucide-react": "^0.263.1",
    "sonner": "^1.0.3",
    "date-fns": "^2.30.0",
    "zod": "^3.22.2",
    "react-hook-form": "^7.45.2",
    "@supabase/supabase-js": "^2.33.1",
    "recharts": "^2.8.0",
    "react-slick": "^0.29.0",
    "react-responsive-masonry": "^2.1.7",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-slick": "^0.23.10",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.5.7",
    "@vitejs/plugin-react": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-pwa": "^0.16.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/user-event": "^14.4.3",
    "msw": "^1.3.2",
    "@playwright/test": "^1.37.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-transform-stub": "^2.0.0",
    "babel-jest": "^29.7.0",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "lighthouse": "^11.0.0",
    "bundle-analyzer": "^0.7.0",
    "jest-axe": "^8.0.0",
    "jest-image-snapshot": "^6.2.0",
    "@axe-core/react": "^4.8.2",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "personal-operating-system",
    "life-management",
    "productivity",
    "goal-tracking",
    "react",
    "typescript",
    "pwa"
  ],
  "author": "Aurum Technology Ltd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aurumtechnology/aurum-life.git"
  },
  "bugs": {
    "url": "https://github.com/aurumtechnology/aurum-life/issues"
  },
  "homepage": "https://aurumlife.com"
}